# Ignore node_modules for Angular frontend
/Frontend/node_modules/

# Ignore Angular build artifacts
/Frontend/dist/
/Frontend/.angular/cache/
/Frontend/.angular/

# Ignore environment files (do not commit secrets!)
/Frontend/src/environments/environment*.ts

# Ignore editor and system files
*.swp
.DS_Store
Thumbs.db
.vscode/
.idea/

# Ignore npm debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# --- .NET (Backend) Ignore Rules ---

# Ignore build results and publish outputs
/Backend/bin/
/Backend/obj/
/Backend/out/
/Backend/TestResults/
/Backend/.vs/
/Backend/.vscode/
/Backend/.user
/Backend/.suo
/Backend/.userosscache
/Backend/.sln.docstates

# Ignore Rider, VS Code, and Visual Studio files (if present)
/Backend/.idea/
/Backend/.vs/
/Backend/*.suo
/Backend/*.user
/Backend/*.userosscache
/Backend/*.sln.docstates

# Ignore ASP.NET generated files
/Backend/wwwroot/
/Backend/node_modules/

# Ignore NuGet packages
/Backend/packages/
/Backend/Packages/

# Ignore logs and secrets
/Backend/*.log
/Backend/appsettings.*.json
!/Backend/appsettings.json

# Ignore coverage reports
/Backend/coverage/
/Backend/coverage*.json

# Ignore environment variables
/Backend/.env

# Ignore deployment artifacts
/Backend/publish/
/Backend/deploy/

# --- Global ignores ---
*.log
*.tmp
*.temp

# End of .gitignore